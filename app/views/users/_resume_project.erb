      <hr />
    </div>

    <div class="col-sm-10 col-md-10 col-lg-10">
      <h3>Projects</h3>
    </div>
  </div>

  <% @projects.each do |project| %>
    <div class="row resume-subsection-row">
      <div class="col-sm-1 col-md-1 col-lg-1 resume-date" style="width:120px">
        1/02â€“12/04
      </div>

      <div class="col-sm-10 col-md-10 col-lg-10">
        <%= link_to project.name, project_path(id: project.id) %>
        <p><%= project.long_description %></p>
      </div>
    </div>
  <% end %>

</div>





